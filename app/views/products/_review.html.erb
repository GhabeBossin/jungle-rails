
<div class="col review-col">
  <div class="card">
    <div class="card-body">

      <ul class="list-group list-group-flush">
        <li class="list-group-item">
            <strong>User review: </strong>
        </li>
        <li class="list-group-item">
          <%= review.description %>
        </li>
        <li class="list-group-item">
          <strong>Rated: </strong>
          <%= review.rating %>/5
        </li>
      </ul>

    </div>
  </div>
</div>